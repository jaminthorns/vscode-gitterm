{
  "name": "git-terminal-history",
  "displayName": "Git Terminal History",
  "description": "Trigger Git history commands for the active editor",
  "publisher": "Jamin Thornsberry",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-terminal-history.fileHistory",
        "title": "Git Terminal History: File History"
      },
      {
        "command": "git-terminal-history.lineHistory",
        "title": "Git Terminal History: Line History"
      }
    ],
    "keybindings": [
      {
        "command": "git-terminal-history.fileHistory",
        "key": "alt+h",
        "when": "editorFocus"
      },
      {
        "command": "git-terminal-history.lineHistory",
        "key": "alt+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
