{
  "name": "gitsquatch",
  "displayName": "GitSquatch",
  "description": "Rich integration between Git CLI and VS Code",
  "publisher": "jaminthorns",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaminthorns/vscode-gitsquatch.git"
  },
  "author": {
    "name": "Jamin Thornsberry"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitsquatch.folderHistory",
        "title": "GitSquatch: Folder History"
      },
      {
        "command": "gitsquatch.fileHistory",
        "title": "GitSquatch: File History"
      },
      {
        "command": "gitsquatch.fileBlame",
        "title": "GitSquatch: File Blame"
      },
      {
        "command": "gitsquatch.fileAtReference",
        "title": "GitSquatch: Open File at Reference"
      },
      {
        "command": "gitsquatch.lineHistory",
        "title": "GitSquatch: Line History"
      },
      {
        "command": "gitsquatch.lineBlame",
        "title": "GitSquatch: Line Blame"
      },
      {
        "command": "gitsquatch.activeFileHistory",
        "title": "GitSquatch: File History"
      },
      {
        "command": "gitsquatch.activeFileBlame",
        "title": "GitSquatch: File Blame"
      },
      {
        "command": "gitsquatch.activeFileAtReference",
        "title": "GitSquatch: Open File at Reference"
      },
      {
        "command": "gitsquatch.selectionHistory",
        "title": "GitSquatch: Selection History"
      },
      {
        "command": "gitsquatch.selectionBlame",
        "title": "GitSquatch: Selection Blame"
      },
      {
        "command": "gitsquatch.selectionSearch",
        "title": "GitSquatch: Selection Search (Pickaxe)"
      },
      {
        "command": "gitsquatch.debugStores",
        "title": "GitSquatch: Debug Stores"
      },
      {
        "command": "gitsquatch.showCommitActions",
        "title": "GitSquatch: Show Commit Actions",
        "icon": "$(git-commit)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "gitsquatch.folderHistory",
          "when": "false"
        },
        {
          "command": "gitsquatch.fileHistory",
          "when": "false"
        },
        {
          "command": "gitsquatch.fileBlame",
          "when": "false"
        },
        {
          "command": "gitsquatch.fileAtReference",
          "when": "false"
        },
        {
          "command": "gitsquatch.lineHistory",
          "when": "false"
        },
        {
          "command": "gitsquatch.lineBlame",
          "when": "false"
        },
        {
          "command": "gitsquatch.showCommitActions",
          "when": "false"
        },
        {
          "command": "gitsquatch.activeFileHistory",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.activeFileBlame",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.activeFileAtReference",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.selectionHistory",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.selectionBlame",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.selectionSearch",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/ && editorHasSelection"
        }
      ],
      "explorer/context": [
        {
          "command": "gitsquatch.folderHistory",
          "group": "40_gitsquatch_folder@0",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "gitsquatch.fileHistory",
          "group": "40_gitsquatch_file@0",
          "when": "!explorerResourceIsFolder"
        },
        {
          "command": "gitsquatch.fileBlame",
          "group": "40_gitsquatch_file@1",
          "when": "!explorerResourceIsFolder"
        },
        {
          "command": "gitsquatch.fileAtReference",
          "group": "40_gitsquatch_file@1",
          "when": "!explorerResourceIsFolder"
        }
      ],
      "editor/title": [
        {
          "command": "gitsquatch.showCommitActions",
          "group": "navigation@0",
          "when": "resourceScheme =~ /^git$|^git-commit|^scm-history-item$/"
        }
      ],
      "editor/title/context": [
        {
          "command": "gitsquatch.fileHistory",
          "group": "10_gitsquatch_file@0",
          "when": "resourceScheme =~ /^file$|^git$/"
        },
        {
          "command": "gitsquatch.fileBlame",
          "group": "10_gitsquatch_file@1",
          "when": "resourceScheme =~ /^file$|^git$/"
        },
        {
          "command": "gitsquatch.fileAtReference",
          "group": "10_gitsquatch_file@1",
          "when": "resourceScheme =~ /^file$|^git$/"
        }
      ],
      "editor/context": [
        {
          "command": "gitsquatch.selectionHistory",
          "group": "10_gitsquatch_selection@0",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.selectionBlame",
          "group": "10_gitsquatch_selection@1",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.selectionSearch",
          "group": "10_gitsquatch_selection@2",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/ && editorHasSelection"
        },
        {
          "command": "gitsquatch.activeFileHistory",
          "group": "11_gitsquatch_file@0",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.activeFileBlame",
          "group": "11_gitsquatch_file@1",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.activeFileAtReference",
          "group": "11_gitsquatch_file@1",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        }
      ],
      "editor/lineNumber/context": [
        {
          "command": "gitsquatch.lineHistory",
          "group": "00_gitsquatch_line@0",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        },
        {
          "command": "gitsquatch.lineBlame",
          "group": "00_gitsquatch_line@1",
          "when": "resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
        }
      ]
    },
    "keybindings": [
      {
        "command": "gitsquatch.activeFileHistory",
        "key": "alt+shift+h",
        "when": "editorFocus && resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
      },
      {
        "command": "gitsquatch.activeFileBlame",
        "key": "alt+shift+b",
        "when": "editorFocus && resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
      },
      {
        "command": "gitsquatch.activeFileAtReference",
        "key": "alt+shift+r",
        "when": "editorFocus && resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
      },
      {
        "command": "gitsquatch.selectionHistory",
        "key": "alt+h",
        "when": "editorTextFocus && resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
      },
      {
        "command": "gitsquatch.selectionBlame",
        "key": "alt+b",
        "when": "editorTextFocus && resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/"
      },
      {
        "command": "gitsquatch.selectionSearch",
        "key": "alt+s",
        "when": "editorTextFocus && resourceScheme =~ /^file$|^git$|^git-commit|^scm-history-item$/ && editorHasSelection"
      }
    ],
    "configuration": {
      "title": "GitSquatch",
      "properties": {
        "gitsquatch.terminalLinks.provideCommitLinks": {
          "type": "string",
          "enum": [
            "always",
            "never"
          ],
          "enumItemLabels": [
            "Always",
            "Never"
          ],
          "enumDescriptions": [
            "Always provide commit links.",
            "Never provide commit links."
          ],
          "default": "always",
          "description": "Whether to provide commit links in the terminal."
        },
        "gitsquatch.terminalLinks.provideCommitRangeLinks": {
          "type": "string",
          "enum": [
            "always",
            "never"
          ],
          "enumItemLabels": [
            "Always",
            "Never"
          ],
          "enumDescriptions": [
            "Always provide commit range links.",
            "Never provide commit range links."
          ],
          "default": "always",
          "description": "Whether to provide commit range links in the terminal."
        },
        "gitsquatch.terminalLinks.provideReferenceLinks": {
          "type": "string",
          "enum": [
            "always",
            "never"
          ],
          "enumItemLabels": [
            "Always",
            "Never"
          ],
          "enumDescriptions": [
            "Always provide reference links.",
            "Never provide reference links."
          ],
          "default": "always",
          "description": "Whether to provide reference links in the terminal."
        },
        "gitsquatch.terminalLinks.provideFileLinks": {
          "type": "string",
          "enum": [
            "always",
            "never",
            "withRevisionContext"
          ],
          "enumItemLabels": [
            "Always",
            "Never",
            "Only when terminal has revision context"
          ],
          "enumDescriptions": [
            "Always provide file links.",
            "Never provide file links.",
            "Only provide file links when terminal has revision context."
          ],
          "default": "always",
          "description": "Whether to provide file links in the terminal by default."
        },
        "gitsquatch.terminalLinks.provideIssueLinks": {
          "type": "string",
          "enum": [
            "always",
            "never"
          ],
          "enumItemLabels": [
            "Always",
            "Never"
          ],
          "enumDescriptions": [
            "Always provide issue links.",
            "Never provide issue links."
          ],
          "default": "always",
          "description": "Whether to provide issue links in the terminal."
        },
        "gitsquatch.showDefault.revision": {
          "type": "string",
          "enum": [
            "editor",
            "terminal"
          ],
          "enumDescriptions": [
            "Editor",
            "Terminal"
          ],
          "default": "editor",
          "description": "How to show a revision by default."
        },
        "gitsquatch.showDefault.revisionRangeDiff": {
          "type": "string",
          "enum": [
            "editor",
            "terminal"
          ],
          "enumDescriptions": [
            "Editor",
            "Terminal"
          ],
          "default": "editor",
          "description": "How to show a revision range diff by default."
        },
        "gitsquatch.showDefault.fileDiffAtRevision": {
          "type": "string",
          "enum": [
            "editor",
            "terminal"
          ],
          "enumDescriptions": [
            "Editor",
            "Terminal"
          ],
          "default": "editor",
          "description": "How to show a diff for a file at a revision by default."
        },
        "gitsquatch.showDefault.fileAtRevision": {
          "type": "string",
          "enum": [
            "editor",
            "terminal"
          ],
          "enumDescriptions": [
            "Editor",
            "Terminal"
          ],
          "default": "editor",
          "description": "How to show a file at a revision."
        },
        "gitsquatch.blame.sameFileMoveCopyDetection": {
          "enum": [
            "off",
            "on"
          ],
          "enumDescriptions": [
            "Do not detect moved/copied lines.",
            "Detect moved/copied lines."
          ],
          "default": "on",
          "description": "In blame commands, detect moved/copied lines within a file."
        },
        "gitsquatch.blame.otherFilesMoveCopyDetection": {
          "enum": [
            "off",
            "sameCommit",
            "sameAndFileCreationCommit",
            "anyCommit"
          ],
          "enumDescriptions": [
            "Do not detect moved/copied lines.",
            "Detect moved/copied lines in the same commit that the line was modified.",
            "Detect moved/copied lines in the same commit that the line was modified and the commit that creates the file.",
            "Detect moved/copied lines in any commit."
          ],
          "default": "sameCommit",
          "description": "In blame commands, detect moved/copied lines from other files."
        },
        "gitsquatch.selectionSearch.modifyEnv": {
          "enum": [
            null,
            "LESS",
            "GITSQUATCH_SEARCH"
          ],
          "enumDescriptions": [
            "Don't modify an environment variable.",
            "Append flags to the LESS environment variable to enable jumping to instances of the search string within less (Git's default pager).",
            "Set the GITSQUATCH_SEARCH environment variable to the search string."
          ],
          "type": "string",
          "default": "LESS",
          "markdownDescription": "How to modify the terminal environment when searching for a string.\n\nThe configured environment variable will only be modified when:\n\n- There is only 1 search string.\n\n- The search string contains no newlines."
        },
        "gitsquatch.gitCommands.folderHistory": {
          "type": "string",
          "default": "git log --stat ${revision} -- '${folder}'",
          "description": "Git command for showing folder history."
        },
        "gitsquatch.gitCommands.fileHistory": {
          "type": "string",
          "default": "git log --follow --patch ${reverseFlags} ${revision} -- '${filename}'",
          "description": "Git command for showing file history."
        },
        "gitsquatch.gitCommands.fileBlame": {
          "type": "string",
          "default": "git blame ${moveCopyDetectionFlags} ${revision} -- '${filename}'",
          "description": "Git command for showing file blame."
        },
        "gitsquatch.gitCommands.lineHistory": {
          "type": "string",
          "default": "git log ${revision} ${fileLineRanges}",
          "description": "Git command for showing history for ranges of lines in a file."
        },
        "gitsquatch.gitCommands.lineBlame": {
          "type": "string",
          "default": "git blame ${moveCopyDetectionFlags} ${revision} ${lineRanges} -- '${filename}'",
          "description": "Git command for showing blame for ranges of lines in a file."
        },
        "gitsquatch.gitCommands.revisionHistory": {
          "type": "string",
          "default": "git log ${reverseFlags} ${revision}",
          "description": "Git command for showing history from a revision."
        },
        "gitsquatch.gitCommands.revisionRangeHistory": {
          "type": "string",
          "default": "git log ${revisionRange}",
          "description": "Git command for showing history for a revision range."
        },
        "gitsquatch.gitCommands.revisionDiff": {
          "type": "string",
          "default": "git diff ${fromRevision} ${toRevision}",
          "description": "Git command for showing a diff between 2 revisions."
        },
        "gitsquatch.gitCommands.showRevision": {
          "type": "string",
          "default": "git show -m ${revision}",
          "description": "Git command for showing a revision."
        },
        "gitsquatch.gitCommands.fileDiffAtRevision": {
          "type": "string",
          "default": "git show -m ${revision} -- '${filename}'",
          "description": "Git command for showing a diff for a file at a revision."
        },
        "gitsquatch.gitCommands.openFileAtRevision": {
          "type": "string",
          "default": "git show ${revision}:'${filename}'",
          "description": "Git command for showing a file at a revision."
        },
        "gitsquatch.gitCommands.stringSearch": {
          "type": "string",
          "default": "git log --patch ${revision} ${searches}",
          "description": "Git command for searching for a string across commits."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "fix": "eslint src --ext ts --fix",
    "test": "vscode-test",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "@types/vscode": "^1.107.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "lookpath": "^1.2.2"
  }
}
